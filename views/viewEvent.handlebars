<div>
    <div>
        <h3>{{name}} Event Name</h3>
        <p>{{description}} Event Description</p>
        <p>{{ starting_date }} Start Date / End Date {{ ending_date }}</p>
        <p>{{created_by}} Created By</p>
    </div>
    <div>
        <h2>Search Tracks</h2>
        <label for="track">Search</label>
        <input type="text" id="track-q">
        <label for="track">Type</label>
        <input type="text" id="track-type">
        <label for="track">Limit</label>
        <input type="text" id="track-limit">
        <button id="search">Search</button>
    </div>
    <div>
        <h2>Songs</h2>
        <table>
            <thead>
                <tr>
                    <th>Song Name</th>
                    <th>Artist</th>
                    <th>Album</th>
                    <th>Photo</th>
                    <th>Preview</th>
                    <th>Add Song</th>
                </tr>
            </thead>
            <tbody id="songs-table-body">
            </tbody>
        </table>
    </div>
</div>

<script src="/js/searchTracks.js"></script>